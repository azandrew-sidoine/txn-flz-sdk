{
    "name": "drewlabs/txn-flz-client",
    "description": "Moov money API driver for txn platform",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": ">=7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Txn\\Flz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Txn\\Flz\\Tests\\": "tests/"
        }
    },
    "authors": [
